{
  "name": "flight",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "enrollment": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "flightNumber":{
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "hour": {
      "type": "number",
      "required": true
    },
    "minutes": {
      "type": "number",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": false,
      "default": "$now"
    },
    "arrival": {
      "type": "date",
      "required": true
    },
    "origin": {
      "type": "string",
      "required": true
    },
    "destination": {
      "type": "string",
      "required": true
    },
    "departure": {
      "type": "date",
      "required": true
    },
    "airline": {
      "type": "string",
      "required": true
    },
    "pilotId": {
      "type": "string",
      "required": true
    },
    "copilotId": {
      "type": "string",
      "required": true
    },
    "eventId": {
      "type": "string",
      "required": false
    }
  },
  "validations": [],
  "acls": [],
  "methods": {}
}
